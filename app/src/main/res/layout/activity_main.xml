<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_aluno"
        android:layout_width="109dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Aluno"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:layout_editor_absoluteX="95dp" />

    <Button
        android:id="@+id/btn_servidor"
        android:layout_width="109dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Servidor"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:layout_editor_absoluteX="95dp" />

    <Button
        android:id="@+id/btn_externo"
        android:layout_width="109dp"
        android:layout_height="46dp"
        android:layout_marginTop="12dp"
        android:text="Externo"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:layout_editor_absoluteX="95dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quantidade:"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="122dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quantidade"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="232dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quantidade"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="347dp" />

    <TextView
        android:id="@+id/txt_quantidade_aluno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="122dp" />

    <TextView
        android:id="@+id/txt_quantidade_servidor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="232dp" />

    <TextView
        android:id="@+id/txt_quantidade_externo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="347dp" />

</android.support.constraint.ConstraintLayout>